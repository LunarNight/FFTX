@{
    Layout = null;
}
@using FFTX.Models
<!DOCTYPE html>

<html>
<head>
    <title>查看详情</title>
</head>
<body>
    <div>
        <table class="table table-bordered" style="margin:20px;padding:10px">
            <tr>
                <th>被封用户id</th>
                <th>用户昵称</th>
                <th>操作管理员id</th>
                <th>封号时间</th>
                <th>封号原因</th>
            </tr>
            <tr>
                <td>@ViewBag.blockUser.User_Id</td>
                <td>@ViewBag.blockUser.User_Name</td>
                <td>@ViewBag.operate.Admin_Id</td>
                <td>@ViewBag.operate.Operate_Time</td>
                <td>@ViewBag.operate.Operate_Reason</td>
            </tr>
        </table>
        点击灰色区域可退出
    </div>
    
</body>
</html>
